{
  "name": "Minimalistic new tab page",
  "description": "Boost productivity by hiding the clutter on the new tab page",
  "version": "1.0",
  "manifest_version": 3,
  "content_security_policy": {
    "default-src": "https://* 'self' 'unsafe-inline'",
    "script-src": "https://* 'self' 'unsafe-inline'"
  },
  "icons": {
    "16": "assets/favicon/16x16.png",
    "32": "assets/favicon/32x32.png",
    "48": "assets/favicon/48x48.png",
    "128": "assets/favicon/128x128.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "geolocation"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
