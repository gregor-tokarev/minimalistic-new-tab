{
  "name": "Minimalistic new tab page",
  "description": "Boost productivity by hiding the clutter on the new tab page",
  "version": "1.0",
  "manifest_version": 3,
  "content_security_policy": {
    "default-src": "https://* 'self' 'unsafe-inline'",
    "script-src": "https://* 'self' 'unsafe-inline'"
  },
  "permissions": [
    "storage",
    "webRequest",
    "geolocation"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
